{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Forms": {
      "Options": { "EnableFormsApi": true }
    },
    "CMS": {
      "DeliveryApi": {
           "Enabled": true,
           "PublicAccess": true,
           "DisallowedContentTypeAliases": ["site", "contentPage", "settings"]
      },
      "Global": {
        "Id": "efc90974-005f-431e-b32e-183160044b20",
        "UseHttps": true,
        "NoNodesViewPath": "~/umbraco/UmbracoWebsite/Deploy/NoNodes.cshtml",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual"
      }
    },
    "Licenses": {
      "Products": {
        "Umbraco.Deploy": "UMBRACO-CLOUD",
        "Umbraco.Forms": "UMBRACO-CLOUD"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=.\\SQLEXPRESS;Database=Nordex;Integrated Security=True;TrustServerCertificate=True;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  },
  "DeepL": {
    "ApiKey": "8b26f2a6-8fc0-4bbd-a925-d4923ce0a905",
    "SourceLanguage": "da",
    "TargetLanguage":  "sv"
  }
}
